#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char name[30],branch[30],section[4],college[30];
    int roll_number;
    scanf("%d\n",&roll_number);
    scanf("%[^\n]%*c",name);
    scanf("%[^\n]%*c",branch);
    scanf("%[^\n]%*c",section);
    scanf("%[^\n]%*c",college);
    
    printf("Roll Number: %d\n",roll_number);
    printf("Name: %s\n",name);
    printf("Branch: %s\n",branch);
    printf("Section: %s\n",section);
    printf("College: %s",college);
    
    

        
    return 0;
}
