#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sum_digits(int num){
   static int sum;
    if(num==0)
        return 0;
    else{
        sum = sum + num%10;
        sum_digits(num/10);
        return sum;
    }
}

int main(){
    int n;
    scanf("%d",&n);
    printf("%d",sum_digits(n));
    return 0;
}
